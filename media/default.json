{
    "name": "hello-world",
    "nodes": [
        {
            "name": "order_db",
            "type": "database"
        },
        {
            "name": "rabbitmq",
            "type": "communicationpattern",
            "ctype": "MessageBroker"
        },
        {
            "name": "gateway",
            "type": "communicationpattern",
            "ctype": "ApiGateway"
        },
        {
            "name": "shipping",
            "type": "service"
        },
        {
            "name": "order",
            "type": "service"
        }
    ],
    "links": [
        {
            "source": "shipping",
            "target": "order_db",
            "timeout": false,
            "type": "runtime"
        },
        {
            "source": "order",
            "target": "order_db",
            "timeout": false,
            "type": "runtime"
        },
        {
            "source": "order",
            "target": "shipping",
            "timeout": false,
            "type": "runtime"
        },
        {
            "source": "gateway",
            "target": "shipping",
            "timeout": false,
            "type": "runtime"
        },
        {
            "source": "shipping",
            "target": "rabbitmq",
            "timeout": false,
            "type": "runtime"
        },
        {
            "source": "order",
            "target": "rabbitmq",
            "timeout": false,
            "type": "runtime"
        }
    ],
    "groups": [
        {
            "name": "edgenodes",
            "type": "edgegroup",
            "members": [
                "gateway",
                "order"
            ]
        }
    ]
}