{
    "name": "hello-world",
    "nodes": [
        {
            "name": "order_db",
            "type": "datastore"
        },
        {
            "name": "order",
            "type": "service"
        },
        {
            "name": "rabbitmq",
            "type": "messagebroker"
        },
        {
            "name": "Api gateway",
            "type": "messagerouter"
        }
    ],
    "links": [
        {
            "source": "order",
            "target": "order_db",
            "timeout": false,
            "dynamic_discovery": false,
            "circuit_breaker": false,
            "type": "interaction"
        },
        {
            "source": "order",
            "target": "rabbitmq",
            "timeout": false,
            "dynamic_discovery": false,
            "circuit_breaker": false,
            "type": "interaction"
        }
    ],
    "groups": [
        {
            "name": "edgenodes",
            "type": "edgegroup",
            "members": [
                "order",
                "Api gateway"
            ]
        },
        {
            "name": "team-primo",
            "type": "squadgroup",
            "members": [
                "order",
                "order_db"
            ]
        },
        {
            "name": "team-secondo",
            "type": "squadgroup",
            "members": [
                "rabbitmq",
                "Api gateway"
            ]
        }
    ]
}